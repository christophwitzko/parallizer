{
  "name": "parallizer",
  "version": "1.0.4",
  "description": "asynchronous functions helper",
  "author": "Christoph Witzko <npm@christophwitzko.com> (http://christophwitzko.com/)",
  "main": "parallizer.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophwitzko/parallizer.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --require should --reporter list --check-leaks",
    "coverage": "./node_modules/.bin/mocha --require blanket --require should --reporter html-cov > coverage.html",
    "coveralls": "./node_modules/.bin/mocha --require blanket --require should --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "coveralls": "^2.11.1",
    "mocha": "^1.18.2",
    "mocha-lcov-reporter": "0.0.1",
    "should": "^4.0.0"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "async",
    "asynchronos",
    "parallel",
    "parallizer"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/christophwitzko/parallizer/raw/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/christophwitzko/parallizer/issues"
  },
  "config": {
    "blanket": {
      "pattern": "parallizer.js"
    }
  },
  "homepage": "https://github.com/christophwitzko/parallizer"
}
